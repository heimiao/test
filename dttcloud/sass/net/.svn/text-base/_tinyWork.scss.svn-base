        font-size:36px;
        margin-top:0;
    }
   .app-btn{
        font-size:20px; 
        color:#fff; 
        margin:0px 10px 10px 0;
    } 
    p{
        color:#fff;
    }
 }   
  
.createApplayContainer{
     @extend .all-container;
