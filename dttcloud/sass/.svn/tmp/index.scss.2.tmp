        .case-ul li:nth-child(1) {
            margin-bottom: 10px;
        }
        .case-ul li:nth-child(2) {
            margin-bottom: 10px;
            background: none;
        }
        .case-num,
        .case-introduce {
            font-size: 12px;
        }
    }
}