 @charset "utf-8"; 
 $grey:#e2e3e6 !default;
 $btn_color:#717171 !default; 
 
.btn_default{
    @include data-btn();
}
.btn_error{
     @include data-btn(auto,auto,#f3856c);
     @include radius(0px);
}
.btn_success{
    @include data-btn(auto,auto,#00703e);
    @include radius(0px);
}
.btn_warning{
     @include data-btn(auto,auto,#e9e335,#fff);
    @include radius(0px);
}
.btn_info{
    @include data-btn(auto,auto,#21a0e8);
    @include radius(0px);
}
