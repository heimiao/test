}
.chanceContainer{
    @extend .all-container;
    background: url(../applyPanel/img/chance.png) 50% 100% no-repeat;  
    .chanceCont{
         @extend .tiny-maxWh;  
         overflow: hidden; 
         .chance-content{
              @extend .app-content; 
              width:58%;
              margin:0 auto 62.3% auto;
               h3{  
                  color:#3a3a3a; 
                }
                p{
                    color:#848e93;
                }
         }  
         .chance-pic{
            width:100%;  
            position:relative; 
            .arry-pic-1{ 
                  position:absolute;
                  top:100px;
                  left:10%;
                  width:20%;  
                  text-align:center; 
                  img{
                      position:relative;
                      width:100%;
                      margin:10px auto;
                  }
                  img:hover{
                      z-index:9999;
                      -webkit-transform:scale(1);
                      -moz-transform:scale(1);  
                       position:absolute;
                       left:40%;
                       top:15%;
                       width:300px; 
                  }
            }
            .arry-pic-2{ 
                  position:absolute;
                  top:30px;
                  right:0;
                  width:80%;  
            }
         } 
         @media screen and(max-width:640px){
             .chance-pic{ 
                   .arry-pic-1{
                        top:90px;
                         img:hover{ 
                           left:-40%;
                           top:-15%;
                           width:150px; 
                      }    
                    } 
                   .arry-pic-2{ 
                        top:10px;
                   }
             }
        } 
        @media screen and(min-width:641px) and(max-width:1000px){
             .chance-pic{ 
                   .arry-pic-1{
                        top:90px;
                        img:hover{ 
                           left:-40%;
                           top:-15%;
                           width:200px; 
                      }  
                    } 
                   .arry-pic-2{ 
                        top:20px;
                   }
             }
        } 
    } 
} 

.addressContainer{
    @extend .all-container;
    background:#f3f3f3;  
    .addressCont{
         @extend .tiny-maxWh;  
         overflow: hidden; 
         .address-title{
              margin:30px auto; 
         }  
         .address-pic{
             width:100%; 
             img{float:left;width:30%;margin:20px 1.66%;  }
         }  
         p{
             text-align:center;
             font-size:18px;
             color:#848e93;
             samp{
                 font-weight:600;
                 color:#ddbc97;
                 font-size:24px;
             }
         }
          @media screen and(max-width:640px){
              .address-title{
                 img{width:100%;margin:0px auto}
                }  
                .address-pic{
                    padding:0 15px;
                  img{
                      float:none;
                      width:100%;
                      margin:10px auto;
                  }
               }  
          }
          @media screen and(min-width:641px) and(max-width:1000px){
              .address-title{
                 img{width:100%;margin:0px auto}
                }  
                .address-pic{ 
                    padding:0 15px;
                    text-align: center;
                  img{
                      display: block;
                      float:none;
                      width:auto;
                      margin:10px auto;
                  }
               }  
          }
    }
}
.theySayContainer{
    @extend .all-container;
    background: url(../img/theySaybg.png) 50% 0 no-repeat;  
    .theySayCont{
         @extend .tiny-maxWh;  
         overflow: hidden; 
         .theySay-content{
              @extend .app-content; 
              width:100%;
              margin:25% auto 10% auto;
              .theyHead{
                 position:relative;
                 height:380px;
                 width:450px;
                 float:right;
                 margin:30px -11% 10px 10px;
                  .theyHeadOne,.theyHeadTwo,.theyHeadThree,.theyHeadFour,.theyHeadFive{
                       position:absolute;
                       width:40%;
                       height:40%; 
                       img{width:100%;height:100%;cursor:pointer}
                  }
                  .theyHeadOne{
                     left:0;
                     top:0;
                   }
                  .theyHeadTwo{
                     left:60%;
                     top:0;
                  }
                  .theyHeadThree{
                       left:0;
                       top:40%;
                  }
                  .theyHeadFour{
                       left:60%;
                       top:40%;
                  }
                  .theyHeadFive{
                      left:30%;
                       top:20%;
                  }
              }  
              .theySayInfo{   
                   background:url(../img/theySayInfobg.png) 50% 0 no-repeat ;
                   padding:2% 0;
                   height:365px;
                   a{
                      text-decoration:none;
                   }
                   h3{
                       color:#2e2e2e;
                       small{
                           display:block;
                           margin:20px auto;
                           font-size:14px;
                           color:#b6b5b5;
                       }
                   };
                   section{
                       width:55%;
                       margin:15% auto;
                       p{
                           color:#dfc09e;
                           font-size:1em;
                       }
                   }
              } 
               @media screen and(max-width:640px){
                   .theyHead{
                         width:95%;
                         float:none;
                         margin:20% auto 0% auto;
                    }
                    .theySayInfo{
                         display: none;
                     }
               }
           @media screen and(min-width:641px) and(max-width:1000px){
                .theyHead{
                         width:60%;
                         float:none;
                         margin:5% auto 0% auto;
                    }
                    .theySayInfo{
                         display: none;
                     }
             }
         }  
    }
}
.bottomBegin{
    @extend .all-container;
    background: url(../img/bottomBeginBg.png) 50% 0 no-repeat;  
    .bottomBeginCont{
         @extend .tiny-maxWh;   
          max-width:800px;
          margin:0 auto; 
         >div{
             margin:5% auto;
         }
          .bottom-dream{
              font-size:24px;
              color:#fff;
              big{
                  font-size:32px;
              }
             .secondFont{
                  font-size:18px;
                  samp{
                      color:#ffad37;
                      font-size:30px;
                  }
              }
          }
          .bottom-join{
              img{
                  width:80%;
                  margin:2% auto;
              }
          }
         }
}

@media screen and(max-width:640px){
    .bottomBegin{
         background:#2a1311;
         .bottom-dream,.bottom-join{
             float:none;
