.phone_section{
    .phoneBg{
         background:#2c2f3b;
         padding-top:150px;
         overflow:hidden;
        .phoneTitle{
            @extend .title; 
            width:430px;
            dl{  
                dt{ 
                    color:#fff;
                }
                dd{ 
                   h1{ 
                       color:#fff;
                       font-weight:100;
                     }
                }
              }
        }
        .phoneDescribe{
          max-width:584px;
          margin:38px auto 0px auto;
          font-size:16px;
          text-align:center;
          color:#848e93; 
        }
        @media screen and(max-width:640px){
              .phoneTitle{
                    width:275px;  
                   .alipayLabIcon{
                        width:45px;
                        height:45px;
                    }  
                    dl{dd{h1{font-size:24px;}}}
             }
             .phoneDescribe{
                 padding:0px 10px;
                 dl{ 
                    dt{  h3{ font-size:18px; } }
                    dd{  padding:10px 15px;font-size:14px;}
                }
            } 
        } 
        .phoneAnimation{  
           /*手机内部样式开始*/
          .phone_phone{max-width:700px;width:90%;margin:30px auto;display: none;} 
         .phoneContainer{
            max-width:780px;
            margin:0px auto;  
            text-align:center; 
            .phone{ 
                width:168px;
                height:344px;  
                margin:50px 40px 30px 40px;
                display: inline-block;
                vertical-align: middle;
//              border:1px solid red;
                background:url(../img_/phone.png) 0 no-repeat;
                .ct { 
                    position: relative; 
                    overflow: hidden;
                     width:134px; 
                     height: 260px; 
                     margin:35px 18px;  
                     cursor: pointer; 
                     background:url(../img_/ct1.jpg);
                      img {   
                            position: absolute;
                            left: 0;
                            top: 0;
                            width:100%;
                            height:100%;
                          }
                 } 
                ul{
                    width:134px; 
                    height: 260px; 
                    margin:35px  18px;  
                    background: #229bdc;
                    overflow: hidden;
                    padding: 5px 0px;   
                    li{
                        list-style-type: none;
                        width: 100%; 
                        line-height:28px; 
                        background: #229bdc; 
                        color: #fff; font-family: "微软雅黑";
                        cursor: pointer; 
                        font-size: 12px;  
                        span{
                            display:inline-block; 
                        }
                        >span{
                            width:8%;
                        }
                        >span~span{
                            text-align:left;
                            width:60%;
                        }
                        transition: all 1s ease;
                        list-style-type:none; 
                    }
                } 
                 .phoneLogo { margin-left: 10px; }
                .phoneContent { margin-left: 20px; } 
             }
             .last_phone ul { 
                background: #253e5d;
                li { 
                    background: #253e5d; 
                    color: #6280a7;
                    >span{width:100%;text-align:left;}
                 } 
                li[data="img"]{ 
                     background: #21344c; 
                     color: #6382a9; 
                      >span{width:10%; }
                      >span~span{width:60%;text-align:left; }
                    }
            } 
            .downloadIDE{
                margin-bottom:100px;
                text-align: center;
                a{
                    display:inline-block;
                }
            } 
         }    
          @media screen and(max-width:780px){
            .phoneContainer{display: none; }
            .phone_phone{display:block; }
          }
        /*手机内部样式结束*/
        }
    }
}
