.flexible_section{
    position:relative;
    z-index:2;
    .flexibleBg{
          @extend .alipayLabBg;  
//        border:1px solid red;
          background:url("../img_/flexibleBg.jpg") no-repeat;
          padding-top:150px;
          .flexibleTitle{
               @extend .title;
               width:470px;  
                dl{  
                    dt{ 
                        color:#fff;
                    }
                    dd{ 
                       h1{ 
                           color:#fff;
                           font-weight:100;
                         }
                    }
                }
          }  
          .flexibleDescribe{
             @extend .alipayLabDescribe;
             dl{
                 margin-top:40px;    
                dt{ 
                    h3{
                         color:#efefef;
                         font-size:20px;
                         font-weight:500px;
                         margin:3px auto; 
                    }
                }
                dd{ 
                    font-size:14px;
                     color:#999999;
                }
            }
        } 
        @media screen and(max-width:640px){
              .flexibleTitle{
                    width:300px;  
                   .alipayLabIcon{
                        width:45px;
                        height:45px;
                    }  
                    dl{dd{h1{font-size:24px;}}}
             }
             .flexibleDescribe{
                 dl{ 
                    dt{  h3{ font-size:18px; } }
                    dd{  padding:0px 15px; }
                }
            } 
        } 
        .flexibleAnimation{
             padding-bottom:30px; 
            .outCircle{  
                text-align:center;
                width:320px;
                height:320px;
                margin:80px auto;
                position:relative; 
                padding-top:65px; 
                img{ 
                    position:absolute;
                    left:0px;
                    top:0px;  
                    width:100%;
                    height:100%;
                     -webkit-animation:rotatename_f 5s linear infinite;
                     -moz-animation:rotatename_f 5s linear infinite;
                     -ms-animation:rotatename_f 5s linear infinite;
                     animation:rotatename_f 5s linear infinite;
                }
                .circleContainer {  
                    width: 250px; 
                    text-align: center; 
                    letter-spacing: -4px;
                    word-spacing: -4px;
                    font-size: 0;
                    margin:0 auto;
                    transition:all 15s 1s linear; 
                    div{
                       a:last-child{
                           border-right:1px solid #fff;
                       }
                    }
                    div:nth-of-type(2){
                       a:last-child{
                           border-top:1px solid #fff;
                       }
                       a:first-child{
                           border-top:1px solid #fff;
                       }
                    }    
                    div:nth-of-type(3){
                       a:last-child{
                           border-top:1px solid #fff;
                       }
                       a:first-child{
                           border-top:1px solid #fff;
                       }
                    }      
                    a {
                        display: inline-block;
                        margin: 0px;
                        height: 0px;
                        width: 22px;
                        height: 22px;
                        font-size: 0;
                        letter-spacing: normal;
                        word-spacing: normal;
                        border-left: 1px solid #fff;
                        border-bottom: 1px solid #fff;
                        border-collapse:collapse;
                        background: #0088cc;
                     }
                     .odd {
//                      background: #0088cc;
//                      -webkit-animation:changeColor 5s 2s ease infinite;
//                      -moz-animation:changeColor 5s 2s ease infinite;
//                      animation:changeColor 5s 2s ease infinite;
                        }
                      .even {
//                      background: #056595;
//                      -webkit-animation:changeColor 5s ease infinite;
//                      -moz-animation:changeColor 5s ease infinite;
//                      animation:changeColor 5s ease infinite;
                        }
                }   
            }   
            @media screen and (max-width:640px){
                .outCircle{
                    width:250px;
                    height:250px;
                    img{width:90%;height:90%;margin-left: 5%;}
                    .circleContainer{
                         margin:-20px auto;
                        a{
                            width:16px;
                            height:16px;
                        }
                    }
                }
            }
        } 
    } 
}
.shortcut_section{
