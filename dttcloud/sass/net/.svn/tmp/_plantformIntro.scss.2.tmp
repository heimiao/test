    /*banner下所有文字水平滑动从左到右，淡入效果展示*/
    @media screen and (min-width: 769px) {
        @keyframes fade-in {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 25%;
                opacity: 1;
            }
        }
        @-webkit-keyframes fade-in {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 25%;
                opacity: 1;
            }
        }
        @-moz-keyframes fade-in {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 25%;
                opacity: 1;
            }
        }
        @-ms-keyframes fade-in {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 25%;
                opacity: 1;
            }
        }
        @-o-keyframes fade-in {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 25%;
                opacity: 1;
            }
        }
        .issue-box {
            animation: fade-in;
            /*动画名称*/
            animation-duration: 1.5s;
            /*动画持续时间*/
            -webkit-animation: fade-in 1.5s;
            /*针对webkit内核*/
            -moz-animation: fade-in 1.5s;
            -ms-animation: fade-in 1.5s;
            -o-animation: fade-in 1.5s;
        }
        @keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-webkit-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-moz-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-ms-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-o-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        .apply-introduce {
            animation: abtnmove;
            /*动画名称*/
            animation-duration: 1.5s;
            /*动画持续时间*/
            -webkit-animation: abtnmove 1.5s;
            /*针对webkit内核*/
            -moz-animation: abtnmove 1.5s;
            -ms-animation: abtnmove 1.5s;
            -o-animation: abtnmove 1.5s;
        }
        /*遮罩层淡入淡出效果展示*/
        .pos {
            background: #080808;
            /*opacity:0.84; 
        filter:alpha(opacity=84);*/
            width: 100%;
            height: 100%;
            border-radius: 2px;
            opacity: 0;
            filter:alpha(opacity=0);
            -webkit-transition: opacity 800ms;
            -moz-transition: opacity 800ms;
            -o-transition: opacity 800ms;
            transition: opacity 800ms;
        }
        .crowd-pos {
            cursor: pointer;
            cursor: hand;
            position: absolute;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            border-radius: 2px;
            /*display: none;*/
            opacity: 0;
            filter:alpha(opacity=0);
            -webkit-transition: opacity 800ms;
            -moz-transition: opacity 800ms;
            -o-transition: opacity 800ms;
            transition: opacity 800ms;
        }
        .posBox:hover .pos {
            opacity: 0.84;
            filter:alpha(opacity=84);
        }
        .crowd_posBox:hover .pos {
            opacity: 0.5;
            filter:alpha(opacity=50);
        }
        .crowd-square-box:hover .crowd-pos {
            opacity: 0.5;
            filter:alpha(opacity=50);
        }
        /*box-shadow渐入渐出效果*/
        .prj-show-li .prj-con-div {
            box-shadow: 0;
            -webkit-transition: box-shadow 800ms;
            -moz-transition: box-shadow 800ms;
            -o-transition: box-shadow 800ms;
            transition: box-shadow 800ms;
        }
        .prj-show-li .prj-con-div:hover {
            box-shadow: 4px 4px 30px 2px #e0e6e9;
        }
        /*鼠标划过合作伙伴logo时效果展示*/
        .parLogo {
            position: relative;
            display: inline-block;
            width: 100%;
        }
        .parLogo img {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            transform: scale(1);
            -webkit-transition: transform 1s;
            -moz-transition: transform 1s;
            -o-transition: transform 1s;
            transition: transform 1s;
        }
        .parLogo:hover img {
            transform: scale(1.5);
        }
