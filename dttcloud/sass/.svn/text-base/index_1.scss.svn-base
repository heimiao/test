 @font-face { 
     font-family:my-font; 
     src: url(../css/abeatbyKaiRegular.otf);
     src: url('../css/abeatbyKaiRegular.otf') format('opentype'), /* IE6-IE8 */
         url('../css/abeatbyKaiRegular.ttf')  format('truetype'), /* Safari, Android, iOS */
         url('../css/abeatbyKaiRegular.svg#my-font') format('svg'); /* Legacy iOS */
      }
  @font-face { 
      font-family:zzf; 
       src: url(../css/myfont.TTF); 
       src: url('../css/myfont.otf') format('opentype'), /* IE6-IE8 */
