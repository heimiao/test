    /*banner下所有文字水平滑动从左到右，淡入效果展示*/
    @media screen and (min-width: 769px) {
        @keyframes fade-in {
            from {
                left: -250%;
                opacity: 0;
            }
            to {
                left: 2%;
                opacity: 1;
            }
        }
        @-webkit-keyframes fade-in {
            from {
                left: -50%;
                opacity: 0;
            }
            to {
                left: 2%;
                opacity: 1;
            }
        }
        @-moz-keyframes fade-in {
            from {
                left: -50%;
                opacity: 0;
            }
            to {
                left: 2%;
                opacity: 1;
            }
        }
        @-ms-keyframes fade-in {
            from {
                left: -50%;
                opacity: 0;
            }
            to {
                left: 2%;
                opacity: 1;
            }
        }
        @-o-keyframes fade-in {
            from {
                left: -50%;
                opacity: 0;
            }
            to {
                left: 2%;
                opacity: 1;
            }
        }
        .issue-box {
            animation: fade-in;
            /*动画名称*/
            animation-duration: 1.5s;
            /*动画持续时间*/
            -webkit-animation: fade-in 1.5s;
            /*针对webkit内核*/
            -moz-animation: fade-in 1.5s;
            -ms-animation: fade-in 1.5s;
            -o-animation: fade-in 1.5s;
        }
        @keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-webkit-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-moz-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-ms-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        @-o-keyframes abtnmove {
            from {
                left: 0;
                opacity: 0;
            }
            to {
                left: 50%;
                opacity: 1;
            }
        }
        .apply-introduce {
            animation: abtnmove;
            /*动画名称*/
            animation-duration: 1.5s;
            /*动画持续时间*/
            -webkit-animation: abtnmove 1.5s;
            /*针对webkit内核*/
            -moz-animation: abtnmove 1.5s;
            -ms-animation: abtnmove 1.5s;
            -o-animation: abtnmove 1.5s;
        }
    }
