    /*遮罩层淡入淡出效果展示*/
    .pos {
        background: #080808;
        /*opacity:0.84; 
         filter:alpha(opacity=84);*/
        width: 100%;
        height: 100%;
        border-radius: 2px;
         @include opacity(0);
        -webkit-transition: opacity 500ms;
        -moz-transition: opacity 500ms;
        -o-transition: opacity 500ms;
        transition: opacity 500ms;
    }
    .posBox {
        position: relative;
        width: 100%;
        height: auto;
        border-radius: 2px;
    }
    .pos {
        cursor: pointer;
        cursor: hand;
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        border-radius: 2px;
        @include opacity(0);
        /*display:none;*/
    }
    .pos p {
        color: #fff;
        position: absolute;
        top: 0;
        left: 0;
        padding: 26px 26px 0;
        line-height: 24px;
        display: none;
    }
    .bg {
        background: #080808;
        opacity: 0.84;
        filter:alpha(opacity=84);
        width: 100%;
        height: 100%;
        border-radius: 2px;
    }
    .posBox:hover .pos {
        opacity: 0.84;
        filter:alpha(opacity=84);
    }
    .posBox:hover .pos p{
        display: block;
    }
