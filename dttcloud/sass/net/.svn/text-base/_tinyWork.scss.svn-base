@charset "utf-8"; 
/*微创业*/
 .tiny-maxWh{
     max-width:1024px;
     margin:0 auto; 
}
.all-container{
    width:100%;
    overflow: hidden;
}
.tinyContainer{
    @extend .all-container;
     background: url(../img/dreambg.png) 50% 100%  no-repeat;
    .dreamCont{  
        @extend .tiny-maxWh;
        margin:10% auto; 
        text-align: center;
        img{
           width:40%;
        }
        .dreamDescribe{
            margin:5% auto;
            img{ width:60%;}
        }
         @media screen and(max-width:640px){
           .dreamCont{  
               img{
                  width:50%;
                }    
            }
            .dreamDescribe{
                        img{ width:80%;}
                    }
         }
    } 
}
.runContainer{
    @extend .all-container;
     background: url(../img/runbg.png) 50% 0  no-repeat;
     .runCont{
         @extend .tiny-maxWh;
           img{
              width:60%;
              margin:10% 5% 10% 35%;
          } 
     } 
     @media screen and(max-width:640px){
         .runCont{
              text-align:center;
              img{
                   width:90%;
                   margin:15% auto;
              }
          }
       }
} 

.mapContainer{
    @extend .all-container;
   background: url(../img/mapbg.png) 50% 100%  no-repeat; 
   .mapCont{
         @extend .tiny-maxWh; 
         text-align: center;
           img{
               width:50%;
               margin:2% auto 10.3% auto;
          } 
     }  
} 
 @media screen and(max-width:1000px){
     .mapContainer{
          background: url(../img/mapbg.png) 50% 100% no-repeat; 
         background-size: auto 100%;  
         .mapCont{
              img{
                   width:50%; 
                   margin:5% auto 15% auto;
              }
         }
     }  
   }
/*公用的字段区域**/
