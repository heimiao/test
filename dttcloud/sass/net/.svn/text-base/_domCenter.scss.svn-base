 } 
.aside-menu{ 
    padding:0px;
    padding-bottom:25px;
    width:100%; 
   hr{
       width:80%;
       margin:15px auto; 
       height:0px;
       border:0px;
       border-bottom:1px dashed #43495a;
   }
   
   nav{   
        .levelMenu_1{ 
               margin-top:15px; 
               margin-bottom:-4px; 
               padding-left:0px;  
               >h3{
                 color:#fff;
                 font-size:14px;
                 font-weight:600; 
                 margin:10px auto;;
                 padding-left:30px;   
               }
               ul{
                   margin:0px;
                   padding:0px;  
                   list-style-type:none; 
                   li{   
                       padding-left:30px; 
                       line-height:36px;
                       &:hover{background:#43495a;} 
                       a{
                           text-decoration:none;
                           font-family: "Microsoft YaHei";
                           -webkit-font-smoothing: antialiased;
                           font-size:12px;
                           color:#afb2bb;
                           display:block;
                       }
                       .show_icon:before,.hide_icon:before{
                            height:5px;
                            width:5px;
                            margin-right:8px;
                            display:inline-block; 
                            content:"";
                            vertical-align:middle;
                        } 
                       .hide_icon:before{ 
                            background: url("//g.369cloud.com/images/static/cloud_school/double_hide.png");
                        }   
                   }  
                   .checked{
                        background:#43495a;
                     }
               } 
           }
           .levelMenu_2{  
                @extend .levelMenu_1; 
                margin:0px;
                >h3{
                    font-size:12px;
                    margin:0px auto;
                    cursor:pointer; 
                    color:#737c86;
                    font-weight:600; 
                    padding-left:30px; 
                    line-height:36px; 
                } 
              .show_icon:before,.hide_icon:before{
                    height:5px;
                    width:5px;
                    margin-right:8px;
                    margin-bottom:2px;
                    display: inline-block; 
                    content: "";
                }
               .show_icon:before,{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/double_show.png");
                }
               .hide_icon:before{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/double_hide.png");
                } 
                ul{
                    display:none;
                    li{
                      padding-left:40px; 
                      .hide_icon:before{ 
                            background: url("//g.369cloud.com/images/static/cloud_school/single_hide.png");
                        } 
                    } 
                }
                .checked{
                   background:#43495a;  
                 } 
           }
           .levelMenu_3{  
                @extend .levelMenu_2;
                display:none;
                 >h3{padding-left:40px;}
                 ul{li{
                        padding-left:50px; 
                    }}
                .show_icon:before,{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/single_show.png");
                 }
               .hide_icon:before{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/single_hide.png");
                }
           }
           .levelMenu_4{  
               @extend .levelMenu_3;
                >h3{padding-left:50px;}
                 ul{li{
                        padding-left:60px; 
                    }}
                 .show_icon:before,{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/single_show.png");
                 }
                .hide_icon:before{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/single_hide.png");
                }
           }  
           .levelMenu_5{  
               @extend .levelMenu_4;
                >h3{padding-left:60px;}
                 ul{li{
                        padding-left:70px; 
                  }}
               .show_icon:before,{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/single_show.png");
                 }
               .hide_icon:before{ 
                    background: url("//g.369cloud.com/images/static/cloud_school/single_hide.png");
                }
           }   
   }  
}   


 /**文档中心banner***/
.sc-banner{ 
    >img{width:100%;
     min-width:1024px;
     min-height:450px;
    }
    position:relative; 
    .bannerCont{
        position:absolute;
        left:0;
        top:30%;
        height:70%;
        width:100%; 
        min-width:1024px; 
        ul{ 
             margin:0;
             padding:0;
             li{
                 list-style:none;
             }
         }
         .load-create{
            width:880px;   
            height:187px;
            margin:0 auto;
             li{ 
                 float:left; 
                 width:48.2%;  
                 height:187px;
                 position:relative;
                 img{
                     position:absolute;
                     left:30%;
                     top:40%; 
                 } 
             }
             li:first-child{
                 margin-right:2%; 
                 background: url("//g.369cloud.com/images/static/cloud_school/csDownload.png") no-repeat;  
             }
             li:first-child+li{ 
                   background: url("//g.369cloud.com/images/static/cloud_school/csApplay.png") no-repeat;
             }
         }
         .menu-icon{
             width:550px;
             margin:1% auto; 
              li{
                 float:left; 
                 width:150px;
                 height:95px;
                 text-align: center; 
                 img{width:50%}
                  h4{
                      font-size:14px;
                      color:#e0f7ff;
                      margin:0;
                      font-weight: 100;
                  }
                  a{text-decoration: none;ourtline:none;}
             }
              li:first-child+li{ 
                  margin:0 7%;
             }
         }
    }
} 


