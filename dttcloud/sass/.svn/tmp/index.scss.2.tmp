/*内容部分*/
.index_blue {
    li {
        list-style-type: none;
    }
    .fl {
        float: left;
    }
    .fr {
        float: right;
    }
    .clearFix:after {
        content: '';
        clear: both;
        display: block;
    }
    .clearFix {
        zoom: 1;
    }
    body {
        font-family: "微软雅黑";
        /*-webkit-font-smoothing: antialiased;*/
    }
    .banner {
        * {
            margin: 0;
            padding: 0;
        }
        width: 100%;
        background: url("../img_new/bg.png");
        position: relative;
        .banner-content {
            max-width: 1200px;
            width: 95%;
            margin: 0 auto;
            text-align: center;
            position: relative;
            .pc_banner {
                width: 100%;
                margin-bottom: -26px;
                display: block;
            }
            .phone_banner {
                display: none;
            }
            .issue-box {
                position: absolute;
                top: 34%;
                left: 2%;
                width: 35%;
                font-size: 14px;
                color: #fff;
                .issue-box-son{
                    text-align: right;
                    margin-right:5%;
                    .app-service {
                        font-size: 30px;
                        @include text-shadow(4px 4px 4px rgba(6,102,205,.75));
                    }
                    .push-teamwork {
                        display: block;
                        text-align: right;
                        //margin-right: 5%;
                        @include text-shadow(4px 4px 4px rgba(6,102,205,.75));
                    }
                }
                
                .issue-ul {
                    position: absolute;
                    width: 120%;
                    left: 5%;
                    left:0%\0;
                    top: 150%;
                    >li {
                        font-size: 14px;
                        float: left;
                    }
                    .appInfo:after {
                        content: '|';
                        color: #87c4ef;
                        margin-right: 10px;
                        padding-left: 10px;
                        height: 18px;
                    }
                }
            }
            @media screen and (max-width: 1230px) {
                .issue-box {
                    font-size: 12px;
                    .issue-box-son{
                        text-align: right;
                        margin-right:5%;
                        .app-service {
                            font-size: 30px;
                        }
                        .push-teamwork {
                            margin-right: 5%;
                        }
                    }
                    
                    .issue-ul {
                        left: 5%;
                        top: 145%;
                        >li {
                            font-size: 12px;
                        }
                    }
                }
            }
            .apply-introduce {
                position: absolute;
                bottom: 10%;
                left: 50%;
                margin-left: -183px;
                text-align: center;
                &:hover {
                    text-decoration: none;
                }
                .aBtn {
                    font-size: 14px;
                    color: #fff;
                    display: inline-block;
                    width: 173px;
                    border: 1px solid #fff;
                    border-radius: 2px;
                    line-height: 42px;
                    margin: 0 5px;
                    &:hover {
                        background: #fff;
                        @include transition(all 800ms);
                        box-shadow: 1px 1px 10px 1px #1297ea;
                        color: #21a0e8;
                        text-decoration: none;
                    }
                }
            }
        }
    }
    .video-box{
        cursor: pointer;
//      height: 484px;
//      background: url(../img_new/page_video.jpg) no-repeat center center;
        width:100%;
