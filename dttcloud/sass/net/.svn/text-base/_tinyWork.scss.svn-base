         } 
    }
}
.theySay-modal{
    position:relative;  
    padding:0;
}
.module-modal .close-modal{
    position:absolute;
    right:-20px;
    top:-20px;
    height:40px;
    width:40px; 
    text-align:center;
    line-height:18px;
    background:url(../img/bgAll.png) 45%  35% no-repeat;
    cursor: pointer;
} 

.module-modal-body{
 overflow: hidden; 
 .theySayIntro{
    a{text-decoration:none}
   .theySayTitle{
        background:#c2c2c2;
        margin-top:60px;
        padding:10px 50px;
        small{
             display: block;
             margin-top:12px;
             color:#fff;
        }
     } 
     p{
       padding:20px 50px;
      }  
        }
  .submitSuccess{
       a{text-decoration:none;outline:none;}
        .submitCont{
            p{
                width:70%; 
                margin:50px auto;
                color:#6d6d6d;
            }
        }
        .submitLink{
            overflow:hidden;
            background:#eee;
            text-align:center;
            div{
              img{
                  margin:10px;
              }
              margin:10px auto;
            }
         }
    } 
} 
.opacity_img{
    opacity:0.5;
    filter:alpha(opacity=50);
}




/*创业申请表LY*/
.tiny-create{
  border: 1px solid #e6e6e6;
  margin-top: 20px;
  padding-bottom: 50px;
}
.tiny-create .title{
  color: #040000;
  font-size: 24px;
  font-family: "Adobe 黑体 Std";
  width: 100%;
  text-align: center;
  margin: 40px 0;
}
.cus-form-group{
  line-height: 40px;
  vertical-align: middle;
  margin: 15px 0px;
}
@media (min-width: 768px) {
  .cus-form-group .cus-label {
    text-align: right;
  } 
} 
.cus-form-group .cus-label{
  height: 34px;
  line-height: 34px;
  //vertical-align: middle;
  //width: 95px;
  font-family: '宋体', Tahoma, Arial;
  font-size: 12px;
  color: #040000;
}
.cus-form-group .cus-div{
  margin-left: 0px;
  font-family: '宋体', Tahoma, Arial;font-size: 12px;color: #040000;
}

.projectType{
  width: 100%;
  line-height: 0px;
}
.projectType a{
  position: relative;
  outline:none;
  left: 0;
  top: -25px;
  cursor: pointer;
  text-decoration: none;
  width: 50px;
  height: 28px;
  line-height: 28px;
  text-align: center;
  vertical-align: middle;
  border: 1px solid #dcdcdc;
  display: inline-block;
  color: #717174;
  font-size: 12px;
  font-family: '宋体', Tahoma, Arial;
  border-radius: 5px; 
}
.projectType a:hover{
  color: #ffffff !important;
  background-color: #21a0e8;
  background-image: none;
}
.projectType label{
  width: 50px;
  height: 30px;
  display: inline-block;
}
.projectType input[type='radio']{
  position: relative;
  left: 0; 
  filter: alpha(opacity=0);
  -moz-opacity: 0;
  opacity: 0;
  z-index: 2;
  width: 50px;
  height: 25px;
  cursor: pointer;
  outline:none;
} 
.projectType .radioChecked{
  color: #ffffff;
  background-color: #21a0e8;
  background-image: none;
}

.projectType .a-active{
  color: #ffffff;
  background-color: #21a0e8;
  background-image: none;
}
.projectType .a-active:hover{
  color: #ffffff;
}  
.custom-inputFile{
  cursor: pointer;
  filter: alpha(opacity=0);
  -moz-opacity: 0;
  opacity: 0;
  font-size: 20px;
  position: relative;
  left: 0;
  top: 0;
  z-index: 2;
  width: 80px;
  height: 30px;
  outline:none;
}
.custom-btn01{
  position: relative;
  top: -25px;
  text-decoration: none;
  cursor: pointer;
  display: block;
  border-radius: 5px;
  color: #000000;
  font-family: '宋体', Tahoma, Arial;
  font-size: 12px;
  text-align: center;
  vertical-align: middle;
  border: 1px solid #707070;
  height: 30px;
  line-height: 30px;
  width: 80px;
  text-align: center;
  text-decoration:none;
  outline:none;
}
.tinyUP-btn{
  color: #ffffff;height: 40px;line-height: 40px;width: 170px;vertical-align: middle;
  text-align: center;font-family: '微软雅黑';font-size: 18px;background-color: #00acf4;
  border-radius: 4px;border: 0
}
.tinyUP-btn:hover,.tinyUP-btn:active,.tinyUP-btn:focus,.tinyUP-btn:visited{
  border-radius: 4px;
  border: 0;
}