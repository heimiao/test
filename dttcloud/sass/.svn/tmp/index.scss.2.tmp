    /*box-shadow渐入渐出效果*/
    /*鼠标划过合作伙伴logo时效果展示*/
    .parLogo {
        position: relative;
        display: inline-block;
        width: 100%;
    }
    .parLogo img {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        transform: scale(1);
        -webkit-transition: transform 1s;
        -moz-transition: transform 1s;
        -o-transition: transform 1s;
        transition: transform 1s;
    }
    .parLogo:hover img {
        transform: scale(1.5);
    }
