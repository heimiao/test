/****文档中心****/ 
.mui-container{
   width:100%;
   margin:0 auto; 
   border:0px;
}
.leftAside{
    position:fixed;
    left:0px;
    top:74px;
    padding-bottom:50px;
    width:288px;
    min-height:400px;
    background:#2f3540;  
    height:100%; 
    .left_icon{
            display: none;
            position:absolute;
            right:-30px;
            top:45%;
            width:30px;
            height:60px;
            cursor:pointer;
        }  
     .menushow{
         background:url("//g.369cloud.com/images/static/cloud_school/menushow.png")
     }
     .menuhide{
         background:url("//g.369cloud.com/images/static/cloud_school/menuhide.png")
     }
}   
.rightCont{ 
    min-height:400px;
    margin-left:288px;
} 
@media screen and(max-width:640px){
    .leftAside{
        width:228px; 
        .left_icon{
          display:block;
        }
      }   
   .rightCont{
       margin-left:0px;
    }   
}
.content{
    padding:25px 30px; 
    img,table{max-width:95%;}
    p{
        text-align:Justify;
        text-justify:inter-ideograph;
    } 
