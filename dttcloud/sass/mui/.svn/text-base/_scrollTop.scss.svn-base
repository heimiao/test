@charset "UTF-8";  
//回到顶部
#scrollUp {
    background:#555;
    color:#fff;  
    text-decoration:none;  
    -webkit-transition:background 200ms linear;
    -moz-transition:background 200ms linear;
    transition:background 200ms linear;
    right:25px;
    line-height:50px;
    text-align:center;
    bottom:25px;
    width:42px;
    height:42px;
    cursor:pointer; 
     background:url("//g.369cloud.com/images/static/cloud_school/uptop.png") no-repeat;
}
