                } 
                .outCircle{
                    position:absolute;
                    left:0px;
                    top:0px;
                    width:100%;
                    height:100%;
                } 
                .marqueecontainer{ 
                    position:absolute;
                    left:28%;
                    top:26%;
                    width:45%;
                    height:50%;
                    opacity:0.5;
                    filter:alpha(opacity=50); 
                }
            }
            @media screen and(max-width:640px){
                    .shortcutcircle{
                        width:250px;
                        height:250px;
                    }
                }
