         .statisticsAnimation{
            .joninus{ 
                margin:3% auto;
                color:#b3b3b4; 
                font-size:14px;
                font-family:gfyh;
                 text-align:center;
            }
            .register{  
                 position:relative;
                width:236px;
                height:58px;
                margin:10px auto;  
                //旋转按钮
                .btn-bg {
                    position: absolute; 
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 45px;
                    text-align: center;
                    font-size: 0;
                    z-index: 100; 
                }
                .btn-bg .bt-wrapper {
                    width: 230px;
                    height: 64px;
                    margin: 0 auto;
                    overflow: hidden;
                    position: relative
                }
                
                .createbtn {
                    font-size: 24px;
                    color:#fff;
                    text-align: center;
                    width: 212px;
                    height: 48px;
                    border: 1px solid #008ee9;
                    box-shadow: 0 0 2px rgba(255, 255, 255, .3);
                    border-radius: 25px;
                    line-height: 46px;
                    cursor: pointer;
                    top: 8px;
                    position: relative;
                    display: block;
                    margin: 0 auto
                }
                .bt-wrapper .l-c {
                    height: 64px;
                    width: 31px;
                    overflow: hidden;
                    position: absolute;
                    left: 0;
                    top: 0
                }
                .bt-wrapper:hover .l-c::after,.bt-wrapper:hover .r-c::after ,.bt-wrapper:hover .line-body::after {
                    -webkit-animation-play-state:paused;
                    -moz-animation-play-state:paused;
                    -o-animation-play-state:paused;
                    animation-play-state:paused;
                }
                .bt-wrapper .l-c::after {
                    content: '\20';
                    width: 63px;
                    height: 63px;
                    position: absolute;
                    top: 1px;
                    left: 0;
                    background: url(../img_/guangbiao.png) no-repeat;
                    -webkit-transform: rotate(-118deg);
                    -moz-transform: rotate(-118deg);
                    -o-transform: rotate(-118deg);
                    transform: rotate(-118deg);
                    -webkit-animation: routateL 6s linear infinite;
                    -moz-animation: routateL 6s linear infinite;
                    -o-animation: routateL 6s linear infinite;
                    animation: routateL 6s linear infinite
                }
                .bt-wrapper .r-c {
                    height: 64px;
                    width: 31px;
                    overflow: hidden;
                    position: absolute;
                    top: 0;
                    right: 0
                }
                .bt-wrapper .r-c::after {
                    content: '\20';
                    width: 62px;
                    height: 63px;
                    position: absolute;
                    top: 0;
                    right: 0;
                    background: url(../img_/guangbiao.png) no-repeat;
                    -webkit-transform: rotate(62deg);
                    -moz-transform: rotate(62deg);
                    -o-transform: rotate(62deg);
                    transform: rotate(62deg);
                    -webkit-animation: routateR 6s linear infinite;
                    -moz-animation: routateR 6s linear infinite;
                    -o-animation: routateR 6s linear infinite;
                    animation: routateR 6s linear infinite
                }
                .bt-wrapper .line-body {
                    height: 64px;
                    width: 168px;
                    left: 31px;
                    position: absolute;
                    top: 0;
                    overflow: hidden
                }
                .bt-wrapper .line-body::after {
                    content: '\20';
                    height: 64px;
                    width: 64px;
                    position: absolute;
                    top: 0;
                    left: -56px;
                    background: url(../img_/guangbiao.png) 0 -63px no-repeat;
                    -webkit-animation: lineFollow 6s linear infinite;
                    -moz-animation: lineFollow 6s linear infinite;
                    -o-animation: lineFollow 6s linear infinite;
                    animation: lineFollow 6s linear infinite
                }  
                
               } 
            .ul{ 
                   text-align:center;
                   margin-top:50px;
                   padding-bottom:50px; 
                a{
                    display:inline-block;
                    list-style-type:none;
                    width:96px;
                    height:96px;
                    margin:0px 20px;
                    position:relative;
                    img{
                        width:100%;
                        height:100%;
                        left:0px;
                        top:0px;
                        position:absolute;
                        z-index:1;
                    }
                    div{
                        position:absolute;   
                        height:0px;
                        width:0px;
                        display:none;
                        z-index:2;
                        left:50%;
                        top:50%; 
                        img{
                            width:100%;
                            height:100%;
                            position:absolute;
                            top:0px;
                            left:0px;
                            z-index:-1;
                        }
                        span{
                            width:80%;
                            height:100%; 
                            display:inline-block;
                            font-size:20px;
                            color:#fff; 
                            margin:50px auto;
                        }
                    }
                } 
            }
            @media screen and(max-width:640px){
                 .ul a{
                     width:50px;
                     margin:0px;
                     height:50px; 
                 }
              }  
        }
    }
}
 
